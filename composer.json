{
    "name": "pascaldevink/shortuuid",
    "description": "PHP 7.4+ library that generates concise, unambiguous, URL-safe UUIDs",
    "type": "library",
    "keywords": [
        "uuid",
        "identifier",
        "guid"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal de Vink",
            "email": "pascal@ticketswap.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "brick/math": "^0.9.1|^0.10|^0.11|^0.12",
        "ramsey/uuid": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "PascalDeVink\\ShortUuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PascalDeVink\\ShortUuid\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
